<!DOCTYPE html>
<html>
	<head>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.5.0/Chart.bundle.min.js"></script>
		<title>Chart</title>
	</head>

	<body>
		<div style="width: 600px;">
			<canvas id="myChart" width="600" height="500"></canvas>
		</div>

		<script>
			var ctx = document.getElementById("myChart");
			var myChart = new Chart(ctx, {
				type: 'line',
				data: {
					datasets: [ [[= model.datasets ]] ]
				},
				options: {
					legend: {
						display: true
					},
					scales: {
						yAxes: [{
							id: 'L',
							position: 'left',
							display: [[= model.showL ]],
							ticks: {
								maxTicksLimit: 9,
							}
						}, {
							id: 'R',
							position: 'right',
							display: [[= model.showR ]],
							ticks: {
								maxTicksLimit: 9,
							}
						}],
						xAxes: [{
							type: 'time',
							time: {
								autoSkip: false,
							},
							ticks: {
								padding: 10,
								maxRotation: 90,
								minRotation: 90,
								maxTicksLimit: 15,
								autoSkip: true,
							},
							scaleLabel: {
								display: true,
								labelString: 'Time ([[= model.tz ]])'
							}
						}]
					}
				}
			});
		</script>
	</body>
</html>